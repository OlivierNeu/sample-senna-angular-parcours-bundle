{
  "config": {
    "imports": {
      "sample-npm-provider": {
        "@angular/animations": "6.1.10",
        "@angular/common": "6.1.10",
        "@angular/compiler": "6.1.10",
        "@angular/core": "6.1.10",
        "@angular/forms": "6.1.10",
        "@angular/http": "6.1.10",
        "@angular/platform-browser": "6.1.10",
        "@angular/platform-browser-dynamic": "6.1.10",
        "@angular/router": "6.1.10",
        "core-js": "2.6.5",
        "rxjs": "6.4.0",
        "zone.js": "0.8.29",
        "jquery": "3.3.1"
      },
      "": {
        "sample-npm-provider": "^1.0.0"
      }
    }
  },
  "process-serially": true,
  "create-jar": {
    "output-dir": "dist",
    "features": {
      "js-extender": true,
      "web-context": "/sample-senna-angular-parcours-bundle",
      "localization": "features/localization/Language",
      "configuration": "features/configuration.json"
    }
  },
  "verbose": true,
  "dump-report": true,
  "exclude": {
    "*": ["__tests__/**/*"]
  },
  "ignore": ["**/js/*.es.js"]
}
